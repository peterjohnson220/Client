  "projects": {
    "admin": {
      "root": "",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/admin",
            "index": "apps/admin/index.html",
            "main": "apps/admin/main.ts",
            "tsConfig": "apps/admin/tsconfig.app.json",
            "polyfills": "apps/admin/polyfills.ts",
            "stylePreprocessorOptions": {
              "includePaths": [
                "assets/scss"
              ]
            },
            "assets": [
              {
                "glob": "**/*",
                "input": "assets/images",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "assets/js",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "apps/admin/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "apps/admin",
                "output": "/"
              },
              {
                "glob": "web.config",
                "input": "apps/admin",
                "output": "/"
              }
            ],
            "styles": [
              "assets/scss/client.scss",
              "apps/admin/styles.scss"
            ],
            "scripts": [
              "assets/js/pendo.js",
              "assets/js/fontawesome-all.min.js"
            ]
          },
          "configurations": {
            "staging": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "apps/admin/../../environments/environment.ts",
                  "with": "apps/admin/../../environments/environment.staging.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "apps/admin/../../environments/environment.ts",
                  "with": "apps/admin/../../environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "admin:build"
          },
          "configurations": {
            "staging": {
              "browserTarget": "admin:build:staging"
            },
            "production": {
              "browserTarget": "admin:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/admin/../../test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "apps/admin/polyfills.ts",
            "tsConfig": "apps/admin/../../tsconfig.spec.json",
            "scripts": [
              "assets/js/pendo.js",
              "assets/js/fontawesome-all.min.js"
            ],
            "styles": [
              "assets/scss/client.scss",
              "apps/admin/styles.scss"
            ],
            "assets": [
              {
                "glob": "**/*",
                "input": "assets/images",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "assets/js",
                "output": "/assets"
              },
              {
                "glob": "**/*",
                "input": "apps/admin/assets",
                "output": "/assets"
              },
              {
                "glob": "favicon.ico",
                "input": "apps/admin",
                "output": "/"
              },
              {
                "glob": "web.config",
                "input": "apps/admin",
                "output": "/"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "./tsconfig.json",
              "./tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
