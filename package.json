  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && ng lint",
    "buildall": "ng build admin && ng build dashboard && ng build legacy-content && ng build peer && ng build auth && ng build community && ng build project && ng build comphub && ng build pf-admin && ng build data-insights && ng build structures && ng build data-management && ng build resources && ng build employees && ng build service && ng build labs",
    "buildallprod": "ng build admin --prod && ng build dashboard --prod && ng build legacy-content --prod && ng build peer --prod && ng build auth --prod && ng build community --prod && ng build project --prod && ng build comphub --prod && ng build pf-admin --prod && ng build data-insights --prod && ng build structures --prod && ng build data-management --prod && ng build resources --prod && ng build employees --prod && ng build service --prod && ng build labs --prod",
    "test": "jest",
    "test-report": "jest --coverage --coverageDirectory=output/coverage/jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:smallbiz-local": "ng build smallbiz --configuration local-universal && ng run smallbiz:server",
    "build:smallbiz-prod": "ng build smallbiz --prod && ng run smallbiz:server:production",
    "compile:universal-server": "tsc -p universal-server/server.tsconfig.json && copyfiles -f universal-server/web.config dist/universal-server",
