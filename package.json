    "tslint": "5.9.1",
    "typescript": "2.6.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "moduleNameMapper": {
      "^environments/(.*)": "<rootDir>/environments/$1",
      "^libs/(.*)": "<rootDir>/libs/$1"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupJest.ts",
    "modulePathIgnorePatterns": [
      "dist"
