{
  "name": "pf-client",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && ng lint",
    "buildallprod": "ng build admin --prod && ng build dashboard --prod && ng build legacy-content --prod && ng build peer --prod && ng build auth --prod && ng build community --prod && ng build project --prod",
    "test": "jest",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:smallbiz-local": "ng build smallbiz --configuration local-universal && ng run smallbiz:server",
    "build:smallbiz-prod": "ng build smallbiz --prod && ng run smallbiz:server:production",
    "compile:universal-server": "tsc -p universal-server/server.tsconfig.json && copyfiles -f universal-server/web.config dist/universal-server",
    "serve:universal": "node dist/universal-server/server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.7",
    "@angular/common": "6.1.7",
    "@angular/compiler": "6.1.7",
    "@angular/core": "6.1.7",
    "@angular/forms": "6.1.7",
    "@angular/http": "6.1.7",
    "@angular/platform-browser": "6.1.7",
    "@angular/platform-browser-dynamic": "6.1.7",
    "@angular/platform-server": "6.1.7",
    "@angular/router": "6.1.7",
    "@mapbox/mapbox-gl-geocoder": "2.3.0",
    "@ng-bootstrap/ng-bootstrap": "3.2.0",
    "@ngrx/effects": "6.1.0",
    "@ngrx/entity": "6.1.0",
    "@ngrx/store": "6.1.0",
    "@ngrx/store-devtools": "6.1.0",
    "@nguniversal/common": "6.1.0",
    "@nguniversal/express-engine": "6.1.0",
    "@nguniversal/module-map-ngfactory-loader": "6.1.0",
    "@progress/kendo-angular-buttons": "4.1.3",
    "@progress/kendo-angular-charts": "3.3.2",
    "@progress/kendo-angular-dateinputs": "3.4.4",
    "@progress/kendo-angular-dropdowns": "3.0.3",
    "@progress/kendo-angular-excel-export": "2.1.2",
    "@progress/kendo-angular-grid": "3.7.1",
    "@progress/kendo-angular-inputs": "3.2.1",
    "@progress/kendo-angular-intl": "1.5.0",
    "@progress/kendo-angular-l10n": "1.2.0",
    "@progress/kendo-angular-layout": "3.1.3",
    "@progress/kendo-angular-upload": "4.1.4",
    "@progress/kendo-data-query": "1.5.0",
    "@progress/kendo-drawing": "1.5.7",
    "@progress/kendo-theme-bootstrap": "2.13.8",
    "bootstrap": "4.1.3",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.7",
    "hammerjs": "2.0.8",
    "lodash.clonedeep": "4.5.0",
    "lodash.isequal": "4.5.0",
    "mapbox-gl": "0.48.0",
    "ng2-dragula": "1.5.0",
    "ng5-slider": "1.1.3",
    "ngx-infinite-scroll": "6.0.1",
    "ngx-mapbox-gl": "2.1.1",
    "perfect-scrollbar": "1.4.0",
    "rxjs": "6.3.2",
    "rxjs-compat": "6.3.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.8.1",
    "@angular/cli": "6.2.1",
    "@angular/compiler-cli": "6.1.7",
    "@angular/language-service": "6.1.7",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.2",
    "@types/mapbox-gl": "0.48.0",
    "@types/node": "10.10.1",
    "codelyzer": "4.4.4",
    "copyfiles": "2.1.0",
    "express": "4.16.3",
    "jest": "23.6.0",
    "jest-preset-angular": "6.0.1",
    "jest-teamcity-reporter": "0.9.0",
    "ngrx-store-freeze": "0.2.4",
    "tslint": "5.11.0",
    "typescript": "2.9.2"
  },
  "jest": {
    "testURL": "http://localhost/",
    "preset": "jest-preset-angular",
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.spec.json"
      },
      "__TRANSFORM_HTML__": true
    },
    "moduleNameMapper": {
      "^environments/(.*)": "<rootDir>/environments/$1",
      "^libs/(.*)": "<rootDir>/libs/$1"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/setupJest.ts",
    "modulePathIgnorePatterns": [
      "dist"
    ],
    "collectCoverageFrom": [
      "**/*.component.ts",
      "**/*.page.ts"
    ],
    "testResultsProcessor": "jest-teamcity-reporter"
  }
}
