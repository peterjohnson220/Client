    "bootstrap": "4.0.0",
    "core-js": "2.5.5",
    "hammerjs": "2.0.8",
    "lodash.clonedeep": "4.5.0",
    "mapbox-gl": "0.44.1",
    "ng2-dragula": "1.5.0",
