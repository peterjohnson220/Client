exports[`CompanyJobDetailComponent should hide a link to download the jdm description via omitting the \`show\` class when no jdm descriptions exists 1`] = `
<pf-company-job-detail
  __ngContext__={[Function LRootView]}
  jdmDescriptionIds={[Function Array]}
  jdmUrl={[Function String]}
  selectedCompanyJob={[Function Object]}
  viewJdmDescriptionClick={[Function EventEmitter_]}
>
  <div
    class="d-flex flex-column h-100"
  >
    <div
      class="pr-3"
    >
      <h5
        class="mb-3 font-weight-bold"
      >
        Company Job
      </h5>
      <div
        class="info-row"
      >
        <label
          class="font-weight-bold w-25 mb-0"
        >
          Title:
        </label>
        <span>
          MockCompanyJobTitle
        </span>
      </div>
      <div
        class="info-row"
      >
        <label
          class="font-weight-bold w-25 mb-0"
        >
          Family:
        </label>
        <span>
          MockJobFamily
        </span>
      </div>
      <div
        class="info-row"
      >
        <label
          class="font-weight-bold w-25 mb-0"
        >
          Level:
        </label>
        <span>
          MockJobLevel
        </span>
      </div>
      <div
        class="info-row"
      >
        <label
          class="font-weight-bold w-25"
        >
          Description:
        </label>
      </div>
    </div>
    <div
      class="info-row border-0 pr-3 company-job-description-container"
    >
      <a
        class="jdm mr-2 d-none"
        href="javascript:void(0)"
      >
        View Full Job Description
      </a>
      <p
        class="font-italic"
      >
         No Job Description Available 
      </p>
    </div>
  </div>
</pf-company-job-detail>
`;

