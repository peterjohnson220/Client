exports[`Admin - Company Admin - User Page - Add Should show the add new user page 1`] = `
<pf-user-page
  apiError$={[Function Store]}
  companyId={[Function Number]}
  loaded$={[Function Store]}
  loading$={[Function Store]}
  route={[Function Object]}
  store={[Function Store]}
  user$={[Function Store]}
  userId="undefined"
  userRoles$={[Function Store]}
>
  <pf-async-container>
    <pf-page-with-title-bar>
      
       Add User 
      
      <button
        class="btn btn-primary"
        id="user-page-save"
      >
        Save
      </button>
      <button
        class="btn btn-secondary"
        id="user-page-cancel"
      >
        Cancel
      </button>
      
      <div
        class="row no-gutters"
      >
        <div
          class="col-lg-6 px-3"
        >
          <pf-user-form />
        </div>
        <div
          class="col-lg-6 px-3 mt-4"
        >
          <div
            class="card"
          >
            <div
              class="card-body"
            >
              <h3
                class="card-title text-center"
              >
                 We've moved! 
              </h3>
              <p
                class="card-text"
              >
                 Tile access and restrictions are now managed with our new User Roles functionality. To access User Roles, click on the dropdown next to your name on the top right of the screen and select Company Administration. In the User Management section, you will see a User Roles link if you have access. Use User Roles to create permission roles and assign your users to those roles. 
              </p>
            </div>
          </div>
        </div>
      </div>
    </pf-page-with-title-bar>
  </pf-async-container>
</pf-user-page>
`;

